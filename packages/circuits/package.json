{
    "name": "@unirep/circuits",
    "version": "1.0.4",
    "main": "dist/src/index.js",
    "scripts": {
        "build": "yarn downloadPtau && yarn buildCircuits && yarn buildSnarks && tsc && yarn copyCircom",
        "downloadPtau": "./scripts/downloadPtau.sh",
        "buildCircuits": "npx ts-node scripts/buildCircuits.ts",
        "buildSnarks": "npx ts-node scripts/buildSnarks.ts",
        "buildTestCircuits": "npx ts-node test/buildTestCircuits.ts",
        "copyCircom": "rm -rf ./dist/zksnarkBuild && cp -rf ./zksnarkBuild ./dist/zksnarkBuild",
        "test": "yarn buildTestCircuits && mocha -r ts-node/register test/*.test.ts --exit",
        "lint": "prettier --write ."
    },
    "repository": "https://github.com/Unirep/circuits.git",
    "author": "vivianjeng <vivi432@yahoo.com.tw>",
    "license": "MIT",
    "dependencies": {
        "@unirep/config": "workspace:*",
        "@unirep/crypto": "workspace:*",
        "argparse": "^2.0.1",
        "circom": "0.5.45",
        "circomlib": "0.5.3",
        "snarkjs": "^0.4.7",
        "typescript": "^4.4.2"
    },
    "devDependencies": {
        "@types/chai": "^4.2.21",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.9.1",
        "chai": "^4.3.4",
        "keyv": "^4.0.3",
        "mocha": "^9.1.1",
        "ts-node": "^10.2.1"
    }
}
