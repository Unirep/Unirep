{
  "name": "unirep",
  "version": "1.0.0",
  "description": "UniRep is a private and nonrepudiable repuation system based on ZKP",
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "contracts": "yarn --silent workspace @unirep/contracts",
    "core": "yarn --silent workspace @unirep/core",
    "circuits": "yarn --silent workspace @unirep/circuits",
    "crypto": "yarn --silent workspace @unirep/crypto",
    "build": "yarn workspaces run build",
    "test": "yarn workspaces run test"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.10",
    "bigint-conversion": "^2.1.12",
    "chai": "^4.2.0",
    "ethereum-waffle": "^3.4.0",
    "keyv": "^4.0.3",
    "shelljs": "^0.8.3",
    "ts-node": "^10.2.1"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "base64url": "^3.0.1",
    "ethers": "^5.4.6",
    "typescript": "^4.5.4"
  },
  "main": "index.js",
  "repository": "https://github.com/Unirep/Unirep",
  "author": "Unirep Team",
  "license": "MIT"
}
